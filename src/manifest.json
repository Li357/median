{
  "manifest_version": 2,
  "name": "Median",
  "version": "0.2.0",
  "author": "Andrew Li",
  "description": "Browser extension that brings LaTeX to Medium",
  "permissions": [
    "https://quicklatex.com/*",
    "https://medium.com/*",
    "webNavigation"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  }
}
