{
  "manifest_version": 2,
  "name": "Median",
  "version": "1.0.0",
  "description": "Browser extension that brings LaTeX to Medium",
  "permissions": [
    "https://quicklatex.com/*",
    "https://medium.com/*",
    "webNavigation"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "48": "/assets/icon.png",
    "96": "/assets/icon@2x.png"
  }
}
